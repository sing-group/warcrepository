<script type="text/javascript">
    function polydea_box_init(wrapper, params) {
    
        jQuery(wrapper + " .more-box-pagination .polydea-box-prev").css('visibility', 'hidden');
        if (params.queried >= params.numposts) {
            jQuery(wrapper + " .more-box-pagination .polydea-box-next").css('visibility', 'hidden');
        }
        
        if (params.feature) {
	        params.featureActive = jQuery(wrapper + ' .more-article-box').eq(0).data('post-id');
	    	jQuery('[data-post-id=' + params.featureActive + ']').addClass('active-thumbnail');
        
	        jQuery(wrapper +' .more-box-slider').on('click', 'a', function(event) {
		        event.preventDefault();
		        params.featureActive = jQuery(this).find('.more-article-box').data('post-id');
		        polydea_box_feature(wrapper, params);
	        });
	    }
    }
    
    function polydea_box_feature(wrapper, params) {
	    jQuery(wrapper + ' .active-thumbnail').removeClass('active-thumbnail');
		jQuery('[data-post-id=' + params.featureActive + ']').addClass('active-thumbnail');
		jQuery(wrapper + ' .pb-featured').animate({opacity: 0, left: -40}, 'fast');
	    jQuery.post(params.ajaxurl, {
		    action: 'polydea_box_feature',
		    postID: params.featureActive,
		    template: params.feature
	    }, function(response) {
		    console.log(response);
		    jQuery(wrapper + ' .pb-featured').stop().html(response).css('left', 40).animate({opacity: 1, left: 0}, 'fast');
		});
    }
    
    function polydea_box_next(wrapper, params, event) {
        event.preventDefault();
        params.offset += params.ppp;
        if (params.offset >= params.numposts) {
            jQuery(wrapper + " .polydea-box-next").css('visibility', 'hidden');
        }
        if (params.offset > params.queried) {
            jQuery(params.loader).addClass('polydea-loading');
            jQuery.post(params.ajaxurl, 
                {
                    action : 'polydea_box_more',
                    postoffset: params.queried,
                    catID: params.cat,
                    ppp: params.ppp,
                    width: params.innerBoxWidth,
                    height: params.innerBoxHeight,
                    boxTemplate : params.boxTemplate,
                },
                function( response ) {
                    params.queried += params.ppp;
                    jQuery(wrapper + ' .more-box-slider').css('width', '+=' + params.innerBoxWidth).append( response );
                    jQuery(wrapper + ' .more-box-slider').animate({left: "-=" + params.innerBoxWidth}, 'fast');
                    jQuery(params.loader).removeClass('polydea-loading');
                }
            );
        } else {
            jQuery(wrapper + ' .more-box-slider').animate({left: "-=" + params.innerBoxWidth}, 'fast');
        }
