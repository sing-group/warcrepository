    <script type="text/javascript">
        $('.social-networks').find('li').find('a').tooltip();
        function Subscribe() {
            $("#subscribeStatus").hide();

            if (!IsEmail($("#subscribeEmail").val())) {
                $("#subscribeStatus").html("<span style='color:red;'>Please enter valid email address</span>");
                $("#subscribeStatus").show();
                return;
            }

            $.get("/Home/Subscribe?email=" + $("#subscribeEmail").val(), function (data) {
                if (data.status == "OK") {
                    $("#subscribeStatus").html("<span style='color:green;'>" + data.message + "</span>");
                    $("#subscribeStatus").show();
                } else {
                    $("#subscribeStatus").html("<span style='color:red;'>" + data.message + "</span>");
                    $("#subscribeStatus").show();
                }
            });
        }


    </script>
