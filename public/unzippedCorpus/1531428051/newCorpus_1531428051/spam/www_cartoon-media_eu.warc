<script type="text/javascript">
	$(document).ready(
		function() {
			var oUp = $('.up'), oDown = $('.down'), oParent, oNews = $('.blockNews');
			oClose = $('.blockDetail-container .close'),
					oElm = null;

			oUp.click(function(e) {
				oParent = $(this).parents('.crt-grid');
				e.preventDefault();
				slideBlock(true, oParent);
			});

			oDown.click(function(e) {
				oParent = $(this).parents('.crt-grid');
				e.preventDefault();
				slideBlock(false, oParent);
			});

			 oNews.each(function(i, elm){
				oElm = elm;
				$(elm).click(function(e){
					e.preventDefault();
					toggleNews(oElm, true);
					// Hande the Ajax Request
					var newsUrl = $(this).find('.blockTxt').data('url'),
				      posting = $.post( newsUrl ),
				      target = $('.newsContainer'),
				      iBlock = $('#content').width()/4;

					target.empty();
					posting.done( function(data) {

						//$(data).appendTo();
						target.append(data);
						 $('.blockDetail').css({
							width  : iBlock*2 - 80,
							height : iBlock*2 - 80
						});
					});
					posting.fail( function(data){
						// Message d'erreur
						//console.log('Ajax request failed!');
					});
		        });
	        });

			oClose.click(function(e) {
				e.preventDefault();
				toggleNews(oElm, false);
			});
		});
</script>
