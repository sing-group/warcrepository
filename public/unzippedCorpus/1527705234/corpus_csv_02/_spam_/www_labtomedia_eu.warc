var accordionContents = document.getElementsByClassName('divContent');

var accordionTogglers1 = document.getElementsByClassName('toggler');
var accordionContents1 = document.getElementsByClassName('accordeon');
var nbr = accordionContents.length;
var nbr1 = accordionContents1.length;
//alert(nbr);

window.onload = function() {
	
//setFooter();

	if(nbr > 0) {
		for ( i=0; i < nbr; i++)
		{
		accordionContents[i].style.display='none';
		accordionTogglers[i].i=i;
		accordionTogglers[i].onclick =  function() {  AfficherDiv(accordionContents, this.i, nbr) };
		}
	}
	
	for ( i=0; i < nbr1; i++)
	{
	accordionContents1[i].style.display='none';
	accordionTogglers1[i].i=i;
	accordionTogglers1[i].onclick =  function() {  AfficherDiv(accordionContents1, this.i, nbr1) };
	}
	
	}

function AfficherDiv(content, div, n)
{
	
	var i=0;
	var etat= content[div].style.display;
	
	
	if(etat=="block")
	{
	content[div].style.display="none";
	return false;
	}
	
	for(i=0;i < n;i++)
	{
	content[i].style.display="none";
	}
	
	content[div].style.display="block";
}
