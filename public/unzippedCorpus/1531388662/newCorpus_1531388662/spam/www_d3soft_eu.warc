<script type="text/javascript">
    function onsuccess() {
        $('.validation-summary-errors').hide();
        $.validator.unobtrusive.parse('form');
        $('#contact-form input[type="text"], #contact-form textarea').val('');
        $('#divSuccess').slideDown(500);
        setTimeout("$('#divSuccess').slideUp(1000);", 3000);

    }
    function onfailure() {
        $('.validation-summary-errors').hide();
        $.validator.unobtrusive.parse('form');
        $('#divFailure').slideDown(500);
    }
    function closediv(e) { $(e).parent().slideUp(500); }

    function resetCaptcha() {
        var contactFormCaptcha = $("img#contactFormCaptcha");
        $("img#contactFormCaptcha").attr("src", $("img#contactFormCaptcha").attr("src") + "?_timeSpan=" + Date.now());
        $('#Captcha').val("");
    }

    function validate() {
        $.ajax({
            method: "GET",
            async: false,
            url: 'http://www.d3soft.eu/Pages/GetCaptchaFromSession',
            data: {},
            success: function (data) {
                var value = $('#Captcha').val();
                if (data == value) {
                    $('#divOwnError').hide();
                    $('form#contact-form').submit();
                }
                else {
                    resetCaptcha();
                    $('.validation-summary-errors').hide();
                    $('#divOwnError span').html('Your transcription of the text from the figure is not correct');
                    $('#divOwnError').slideDown(500);
                    return false;
                }
            }
        });
    }

</script>

